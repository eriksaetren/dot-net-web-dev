<!DOCTYPE html>
<html>
<head>
    <!-- Google Tag Manager -->
    <script>
        (function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
        new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
        j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
        'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
        })(window, document, 'script', 'dataLayer', 'GTM-TLKRHJ3');
    </script>
    <!-- End Google Tag Manager -->

    <link href="https://fonts.googleapis.com/css?family=Droid+Serif" rel="stylesheet">
    
    <!-- Favicon Stuff -->
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="/manifest.json">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="theme-color" content="#ffffff">

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-91814522-1', 'auto');
      ga('send', 'pageview');

    </script>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="@ViewBag.Description" />
    <link rel="stylesheet" href="~/Content/SecondaryLayoutStyles.css" />

    <title>@ViewBag.Title - PlanSimply By CJ</title>
    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")*@

    
</head>
<body>
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-TLKRHJ3"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->

    <h1>Plan Simply by CJ Wedding Planner Tucson</h1>
    <div id="mainNav" class="navbar navbar-fixed-top">
        <div class="container" >
            <div class="navbar-header">
                <button id="ddButton" type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar navIcon"></span>
                    <span class="icon-bar navIcon"></span>
                    <span class="icon-bar navIcon"></span>
                </button>
                <a id="topLogoLink" href="/" class="navbar-brand navLi" ><img id="topLogoImg" alt="PlanSimply by CJ Logo" src="~/Content/Images/Logo/Logo1_WEB.png" class="img" /></a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a class="text-right navLi" href="/wedding/services">Services</a></li>
                    <li><a class="text-right navLi" href="/wedding/gallery">Gallery</a></li>
                    <li><a class="text-right navLi" href="/wedding/contact">Contact</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div id="bodyContainer" class="container body-content">
        @RenderBody()
        
        <footer>
            <div class="container">
                <hr />
                <div class="row">
                    <div class="col-md-1">
                    </div>
                    <div class="col-md-3 text-center">
                        <a class="col-md-12 footLink" href="/wedding/services">Services</a>
                    </div>
                    <div class="col-md-2 text-center">
                        <a class="col-md-12 footLink" href="/wedding/gallery">Gallery</a>
                    </div>
                    <div class="col-md-2 text-center">
                        <a class="col-md-12 footLink" href="/wedding/contact">Contact</a>
                    </div>
                    <div class="col-md-3 text-center">
                        <span class="footLink">&copy; @DateTime.Now.Year - PlanSimply by CJ&trade;</span>
                    </div>
                    <div class="col-md-1">
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xs-3"></div>
                    <div class="col-xs-6">
                        <a href="/"><img alt="PlanSimply by CJ Logo" src="~/Content/Images/Logo/Logo2_WEB.png" class="img img-responsive" /></a>
                    </div>
                    <div class="col-xs-3"></div>
                </div>
                <hr />
                <div class="row">
                    <div class="col-md-4"></div>
                    <div class="col-md-4">
                        <a rel="" target="_blank" href="http://tech-simply.com">
                            <p id="devText" class="text-center">
                                Created by Tech Simply
                            </p>
                        </a>
                    </div>
                    <div class="col-md-4"></div>
                </div>
                <br />
                <br />
                <br />
            </div>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
